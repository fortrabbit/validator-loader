    },
    "require-dev": {
        "mockery/mockery": "*",
        "phpunit/phpunit": "*",
        "symfony/yaml": "2.4.*"
    },
    "autoload": {
        "psr-0": {
            "Frbit\\ValidatorLoader\\": "src/"
        }
    }
}