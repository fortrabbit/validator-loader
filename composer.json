{
    "name": "frbit/validator-loader",
    "version": "0.1.1",
    "license": "MIT",
    "description": "Load Laravel validator definitions from files or directories",
    "keywords": ["laravel", "validator", "validation"],
    "author": {
        "name": "Ulrich Kautz",
        "email": "uk@fortrabbit.com"
    },
    "require": {
        "illuminate/filesystem": "4.2.*",
